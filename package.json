    "stylelint-scss": "^3.19.0",
    "stylelint-webpack-plugin": "^2.1.1",
    "ts-loader": "^8.0.17",
    "typescript": "^4.2.2",
    "url-loader": "^4.1.1",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "assert": "^2.0.0",
    "bootstrap": "^5.0.0-beta2",
    "classnames": "^2.2.6",
