{
  "name": "jet-edit",
  "version": "0.0.1-alpha.0",
  "description": "Webinterface for Jet",
  "author": "Fahri Sönmez",
  "homepage": "https://github.com/HBM/jet-edit#readme",
  "license": "",
  "contributors": [
    "Kai Schreyer <kai.schreyer@hbm.com>",
    "Fahri Sönmez <fahri.soenmez@hbm.com>"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HBM/jet-edit.git"
  },
  "bugs": {
    "url": "https://github.com/HBM/jet-edit/issues"
  },
  "scripts": {
    "clean": "rm -fr dist test-builds coverage",
    "test": "npm run clean && node node_modules/typescript/bin/tsc --project tsconfig.json --outDir \"./test-builds/\" --sourceMap && node node_modules/jest/bin/jest.js --coverage",
    "tsc": "node node_modules/typescript/bin/tsc --project tsconfig.json --noEmit",
    "bundle": "npm run clean && node node_modules/webpack/bin/webpack.js --config webpack/prod.config.js",
    "dev": "node node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack/dev.config.js",
    "prettier": "node node_modules/prettier/bin-prettier.js --write \"./**/*.{js,jsx,ts,tsx,json,html}\"",
    "prettier:ci": "node node_modules/prettier/bin-prettier.js --list-different \"./**/*.{js,jsx,ts,tsx,json,html}\"",
    "eslint": "node node_modules/eslint/bin/eslint.js --max-warnings 0 \"{app,tests}/**/*.{js,jsx,ts,tsx}\"",
    "stylelint:ci": "node node_modules/stylelint/bin/stylelint.js \"app/**/*.scss\"",
    "stylelint": "node node_modules/stylelint/bin/stylelint.js --fix \"app/**/*.scss\""
  },
  "devDependencies": {
    "@testing-library/react": "^10.4.7",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/jest": "^26.0.4",
    "@types/node": "^14.0.23",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "@types/react-router-dom": "^5.1.5",
    "@types/webpack": "^4.41.21",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "autoprefixer": "^9.8.5",
    "compression-webpack-plugin": "^4.0.0",
    "css-loader": "^3.6.0",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "postcss-loader": "^3.0.0",
    "prettier": "^2.0.5",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "stylelint": "^13.6.1",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-scss": "^3.18.0",
    "stylelint-webpack-plugin": "^2.1.0",
    "ts-loader": "^8.0.1",
    "typescript": "^3.9.7",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "bootstrap": "^5.0.0-alpha1",
    "node-jet": "^2.0.0",
    "react": "^16.13.1",
    "react-bootstrap": "^1.2.2",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0"
  }
}
